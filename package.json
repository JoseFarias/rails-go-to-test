{
	"name": "rails-go-to-test",
	"displayName": "Rails go to test",
	"description": "Switch between code and test files in Rails",
	"version": "0.0.1",
	"publisher": "SimonBo",
	"repository": {
		"type": "git",
    "url": "https://github.com/SimonBo/rails-go-to-test"
	},
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:rails-go-to-test.goToTest"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "rails-go-to-test.goToTest",
            "title": "Go to test"
		}],
		"keybindings": [
      {
        "command": "rails-go-to-test.goToTest",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t",
        "when": "editorTextFocus"
      }
    ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.45.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
		"mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    }

}
